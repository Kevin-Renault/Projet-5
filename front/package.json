{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:e2e": "ng serve --proxy-config proxy.conf.json --port 4201",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:junit": "cypress run --reporter mocha-junit-reporter --reporter-options mochaFile=cypress/results/junit-[hash].xml",
    "pretest:e2e": "node -e \"require('fs').mkdirSync('cypress/results', { recursive: true })\"",
    "test:e2e": "start-server-and-test start:e2e http://localhost:4201 cypress:run:junit --"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.18",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^19.2.18",
    "@angular/compiler": "^19.2.18",
    "@angular/core": "^19.2.18",
    "@angular/forms": "^19.2.18",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^19.2.18",
    "@angular/platform-browser-dynamic": "^19.2.18",
    "@angular/router": "^19.2.18",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.19",
    "@angular/cli": "~19.2.19",
    "@angular/compiler-cli": "^19.2.18",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^30.0.0",
    "cypress": "^15.10.0",
    "jasmine-core": "~4.2.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^16.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "mocha-junit-reporter": "^2.2.1",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.8.3"
  }
}